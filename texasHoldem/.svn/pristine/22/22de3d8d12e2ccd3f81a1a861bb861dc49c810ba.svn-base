
public class Kasi {

    private Kortti[] kasi;

    public Kasi() {
        kasi = new Kortti[2];
    }
    
    public void lisaaKortti(Kortti kortti){
        for(int i=0; i<kasi.length; i++){
            if(kasi[i]==null){
                kasi[i]=kortti;
                return;
            }
        }
    }
    
    public Kortti getKortti(int i){
        return kasi[i];
    }
    
}
